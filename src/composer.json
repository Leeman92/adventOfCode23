{
  "require": {
    "vlucas/phpdotenv": "^5.5",
    "ext-gmp": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.37",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10",
    "symfony/var-dumper": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "App/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
